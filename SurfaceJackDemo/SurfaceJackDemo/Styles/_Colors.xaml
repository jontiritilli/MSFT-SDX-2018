<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- system default -->
    <SolidColorBrush x:Key="ThemeControlForegroundBaseHighBrush" Color="{ThemeResource SystemBaseHighColor}"></SolidColorBrush>

    <!-- ColoringBook control -->
    <Color x:Key="ColoringBookColorRed">#FFe53061</Color>
    <Color x:Key="ColoringBookColorBlue">#FF0c6290</Color>
    <Color x:Key="ColoringBookColorTeal">#FF98d8d8</Color>
    <Color x:Key="ColoringBookColorOrange">#FFffb367</Color>
    <Color x:Key="ColoringBookColorPurple">#FF9765f5</Color>

    <!-- MusicBar control-->
    <Color x:Key="MusicBarProgressBarBackground">#FF737373</Color>
    <Color x:Key="MusicBarProgressBarForeground">#FF333333</Color>
    
</ResourceDictionary>
